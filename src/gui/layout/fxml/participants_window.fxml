<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<VBox prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="gui.controllers.RDVController">
    <stylesheets>
        <URL value="@../css/rdv_window.css"/>
    </stylesheets>
    <MenuBar VBox.vgrow="NEVER">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" text="New"/>
            <MenuItem mnemonicParsing="false" text="Openâ€¦"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Quit"/>
        </Menu>
    </MenuBar>
    <TableView>
        <columns>
            <TableColumn prefWidth="76.0" text="First name"/>
            <TableColumn prefWidth="74.0" text="Last name"/>
            <TableColumn prefWidth="74.0" text="Phone number"/>
            <TableColumn prefWidth="74.0" text="Email"/>
            <TableColumn prefWidth="74.0" text="Address"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
    <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button mnemonicParsing="false" text="Exit"/>
            <Button mnemonicParsing="false" text="Add new"/>
            <Button mnemonicParsing="false" text="Delete entry"/>
        </buttons>
        <padding>
            <Insets right="10.0"/>
        </padding>
        <effect>
            <Blend/>
        </effect>
    </ButtonBar>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
        </padding>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Ready" HBox.hgrow="ALWAYS">
            <font>
                <Font size="11.0" fx:id="x3"/>
            </font>
            <textFill>
                <Color blue="0.625" green="0.625" red="0.625" fx:id="x4"/>
            </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS"/>
        <Label font="$x3" maxWidth="-1.0" text="Participants' directory" textFill="$x4" HBox.hgrow="NEVER"/>
    </HBox>
</VBox>
